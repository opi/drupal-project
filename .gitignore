# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/web/translations/contrib/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore local services
/web/sites/*/local.services.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore private file directory
/private/*
!/private/.htaccess

# Ignore SimpleTest multi-site environment
/web/sites/simpletest

# Ignore files generated by common IDEs
/.idea/
/.vscode/
*.code-workspace
*.sublime-project
*.sublime-workspace

# Ignore .env files as they are personal
/.env
/.editorconfig
/.gitattributes

# Ignore Apache config file
/web/.htaccess

# Ignore Apache password file
.htpasswd
